{
  "name": "tarely-web",
  "version": "1.0.0",
  "description": "tarely web client app",
  "engines": {
    "node": "8.11.1",
    "npm": "6.0.0"
  },
  "dependencies": {
    "antd": "^3.4.1",
    "react-scripts": "1.0.17",
    "classnames": "^2.2.5",
    "emoji-js": "^3.4.1",
    "emoji-mart": "^2.6.1",
    "geolib": "^2.0.24",
    "isomorphic-fetch": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.10",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "rc-input-number": "^4.0.12",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-highlight-words": "^0.11.0",
    "react-icons": "^2.2.7",
    "react-places-autocomplete": "^7.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-modal": "^1.3.5",
    "react-router-transition": "^1.2.1",
    "react-swipe-card": "^0.1.4",
    "react-swipeable-views-core": "^0.12.11",
    "react-textarea-autosize": "^7.0.3",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "socket.io": "^2.1.1",
    "validator": "^10.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "lint": "eslint --ext=js --ext=jsx src/*",
    "lint:fix": "eslint --ext=js --ext=jsx src/* --fix",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
